static void handle_OP_PADSV(PADOP *op) {
    CV *cv = find_runcv(NULL);
    AV *names = (AV *) *av_fetch(CvPADLIST(cv), 0, 0);
    last_seen_identifier = get_identifier(SvPV_nolen(AvARRAY(names)[PL_op->op_targ]));
}

